<div class="table-responsive">
  <table class="table table-striped jambo_table bulk_action">
    <thead>
    <tr class="headings">
      <th>
        <input type="checkbox" id="check-all" class="flat">
      </th>
      <th class="column-title"><%= t('admin.system_admins.items.name') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.email') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.birthday') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.gender') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.zip_code') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.address') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.tel1') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.tel2') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.memo') %></th>
      <th class="column-title"><%= t('admin.system_admins.items.user_info') %></th>
      <th class="column-title no-link last" colspan="3"><span class="nobr"><%= t('admin.system_admins.items.controls') %></span>
      </th>
      <th class="bulk-actions" colspan="13">
        <a class="antoo" style="color:#fff; font-weight:500;"><%= t('admin.elements.buttons.bulc_action') %> ( <span class="action-cnt"> </span> ) <i class="fa fa-chevron-down"></i></a>
      </th>
    </tr>
    </thead>

    <tbody>
    <% system_admins.each do |system_admin| %>
        <tr class="even pointer">
          <td class="a-center">
            <input type="checkbox" class="flat" name="table_records">
          </td>
          <td><%= system_admin.first_name %></td>
          <td><%= system_admin.user.email %></td>
          <td><%= system_admin.birthday %></td>
          <td><%= system_admin.gender.name %></td>
          <td><%= system_admin.zip_code %></td>
          <td><%= system_admin.address %></td>
          <td><%= system_admin.tel1 %></td>
          <td><%= system_admin.tel2 %></td>
          <td><%= system_admin.memo %></td>
          <td><%= set_button_show('', [:admin, :members, system_admin]) %>
          <td><%= set_button_edit('', edit_admin_members_system_admin_path(system_admin)) %>
          <td><%= set_button_delete('', [:admin, :members, system_admin]) %>
        </tr>
    <% end %>
    </tbody>
  </table>

</div>